<!DOCTYPE html>
<html>
<head>
    <title>@if(string.IsNullOrWhiteSpace(ViewBag.Title)) {
               <text>Advertising Creative</text>
           } else { @ViewBag.Title }
                - Matt Baines</title>
    
    <link href="@Url.Content("~/Content/UITheme/uitheme.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/gridNavigation.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/bootstrap.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    @RenderSection("StyleSheet", required: false)

    <script src="@Url.Content("~/Scripts/jquery-1.8.3.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.23.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.wijmo-open.all.2.2.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GridNavigationEffects/jquery.easing.1.3.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GridNavigationEffects/jquery.gridnav.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/GridNavigationEffects/jquery.mousewheel.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/bootstrap.min.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/cufon-yui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Brushed_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/THURSTON_erc_400.font.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/Sketch_Block_400.font.js")" type="text/javascript"></script>

    <script src="@Url.Content("~/Scripts/Application/Application.js")" type="text/javascript"></script>
    @RenderSection("JavaScript", required: false)
</head>
<body>
@if (TempData["Notice"] != null)
{
    <div id="notice_dialog" style="display:none" title="Notice">
        @Html.Raw(TempData["Notice"].ToString())
    </div>
}
        @RenderBody()
</body>
</html>
